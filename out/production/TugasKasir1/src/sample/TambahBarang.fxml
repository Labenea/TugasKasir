<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.202-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.TambahBarang">
   <children>
      <BorderPane prefHeight="400.0" prefWidth="600.0" stylesheets="@MainScreen.css">
         <center>
            <TableView fx:id="tableTambah" onMouseClicked="#selectedTable" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
              <columns>
                <TableColumn fx:id="colNo" prefWidth="41.0" resizable="false" text="No" />
                <TableColumn fx:id="colKode" prefWidth="114.0" resizable="false" text="Kode Barang" />
                  <TableColumn fx:id="colNama" prefWidth="215.0" resizable="false" text="Nama Barang" />
                  <TableColumn fx:id="colSatuan" prefWidth="76.0" resizable="false" text="Satuan" />
                  <TableColumn fx:id="colHarga" prefWidth="68.0" resizable="false" text="Harga" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </center>
         <top>
            <Pane prefHeight="65.0" prefWidth="600.0" styleClass="kasirColor" BorderPane.alignment="CENTER">
               <children>
                  <TextField fx:id="searchBarang" layoutX="158.0" layoutY="22.0" onKeyReleased="#searchOnKeyPressed" prefHeight="26.0" prefWidth="407.0" />
                  <Label layoutX="36.0" layoutY="19.0" styleClass="transaksi" text="Cari Barang" />
               </children>
            </Pane>
         </top>
         <right>
            <Pane prefHeight="335.0" prefWidth="34.0" styleClass="kasirColor" BorderPane.alignment="CENTER" />
         </right>
         <left>
            <Pane prefHeight="335.0" prefWidth="34.0" styleClass="kasirColor" BorderPane.alignment="CENTER" />
         </left>
         <bottom>
            <Pane prefHeight="96.0" prefWidth="600.0" styleClass="kasirColor" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="namaBarang" layoutX="35.0" layoutY="8.0" styleClass="totalItem" text="Nama Barang" />
                  <Label layoutX="35.0" layoutY="48.0" styleClass="transaksi" text="Jumlah" />
                  <TextField fx:id="jumlahInput" layoutX="118.0" layoutY="50.0" onKeyPressed="#jumlahInputOnKeyPressed" prefHeight="26.0" prefWidth="282.0" />
                  <Button fx:id="tambahButton" layoutX="417.0" layoutY="14.0" mnemonicParsing="false" onAction="#tambahButtonAction" prefHeight="62.0" prefWidth="151.0" styleClass="tambahButton" text="Tambah " textFill="WHITE" />
               </children>
            </Pane>
         </bottom>
      </BorderPane>
   </children>
</AnchorPane>
